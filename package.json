        "telegraf": "^4.8.1",
        "ydb-sdk": "3.4.4"
    },
    "devDependencies": {
        "@types/node": "^17.0.31",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "nodemon": "^2.0.16",
        "ts-node": "^10.7.0",
        "typescript": "^4.7.4"
