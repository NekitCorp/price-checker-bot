{
    "name": "price-checker-bot",
    "private": true,
    "version": "1.0.0",
    "engines": {
        "npm": ">=8.0.0 <9.0.0",
        "node": ">=16.0.0 <17.0.0"
    },
    "scripts": {
        "dev:start-bot": "nodemon --signal SIGHUP --files dev/start-telegram-bot.ts",
        "dev:migrate-test-data": "ts-node --files dev/migrate-test-data.ts",
        "dev:trigger": "ts-node --files dev/trigger.ts",
